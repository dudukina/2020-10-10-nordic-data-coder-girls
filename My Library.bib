@article{larsen_socioeconomic_2011,
	title = {Socioeconomic position and lifestyle in relation to breast cancer incidence among postmenopausal women: {A} prospective cohort study, {Denmark}, 1993–2006},
	volume = {35},
	issn = {1877-7821},
	shorttitle = {Socioeconomic position and lifestyle in relation to breast cancer incidence among postmenopausal women},
	url = {http://www.sciencedirect.com/science/article/pii/S187778211000216X},
	doi = {10.1016/j.canep.2010.12.005},
	abstract = {Background: In Denmark, the incidence of breast cancer is higher among women with higher socioeconomic position. We investigated whether differences in exposure to certain risk factors contribute to this gradient, as measured from education, income and occupation. Methods: We conducted a cohort study of 23111 postmenopausal women aged 50–65 years who were enrolled in the prospective Danish ‘Diet, Cancer and Health’ study between 1993 and 1995. At baseline, all women filled in a questionnaire on lifestyle and food frequency. The results were analysed in Cox proportional hazard models. Results: Part of the association with socioeconomic position is due to the potential mediators reproductive pattern, use of hormone replacement therapy and alcohol consumption. After simultaneous adjustment for these factors, the hazard ratios were 1.06 (95\% confidence interval [CI], 0.88–1.27) for women with higher education and 1.07 (95\% CI, 0.85–1.34) for women with higher income. The HR ratio for women working as higher officials when compared with unskilled workers was 1.23 (0.96–1.59). Conclusion: The results support the hypothesis that the higher incidence of breast cancer among socially advantaged women is mediated partly by differences in exposure to reproductive factors, hormone replacement therapy and alcohol.},
	language = {en},
	number = {5},
	urldate = {2020-06-12},
	journal = {Cancer Epidemiology},
	author = {Larsen, Signe Benzon and Olsen, Anja and Lynch, John and Christensen, Jane and Overvad, Kim and Tjønneland, Anne and Johansen, Christoffer and Dalton, Susanne Oksbjerg},
	month = oct,
	year = {2011},
	keywords = {Breast cancer, Lifestyle factors, Socioeconomic position},
	pages = {438--441},
	file = {ScienceDirect Full Text PDF:/Users/elenadudukina/Zotero/storage/SKKD7DYJ/Larsen et al. - 2011 - Socioeconomic position and lifestyle in relation t.pdf:application/pdf;ScienceDirect Snapshot:/Users/elenadudukina/Zotero/storage/S7JQ9PGI/S187778211000216X.html:text/html}
}

@article{mihor_socioeconomic_2020,
	title = {Socioeconomic inequalities in cancer incidence in {Europe}: a comprehensive review of population-based epidemiological studies},
	volume = {54},
	shorttitle = {Socioeconomic inequalities in cancer incidence in {Europe}},
	url = {https://content.sciendo.com/view/journals/raon/54/1/article-p1.xml},
	doi = {10.2478/raon-2020-0008},
	abstract = {{\textless}section class="abstract"{\textgreater}{\textless}h2 class="abstractTitle text-title my-1" id="d712e2"{\textgreater}Abstract{\textless}/h2{\textgreater}{\textless}div id="j\_raon-2020-0008\_s\_004\_w2aab3b8c76b1b7b1aab1c18b1Aa" class="section"{\textgreater}{\textless}h3 class="abstractTitle text-title my-1" id="d712e5"{\textgreater}Background{\textless}/h3{\textgreater}{\textless}p{\textgreater}Since the end of the previous century, there has not been a comprehensive review of European studies on socioeconomic inequality in cancer incidence. In view of recent advances in data source linkage and analytical methods, we aimed to update the knowledge base on associations between location-specific cancer incidence and individual or area-level measures of socio-economic status (SES) among European adults.{\textless}/p{\textgreater}{\textless}/div{\textgreater}{\textless}div id="j\_raon-2020-0008\_s\_005\_w2aab3b8c76b1b7b1aab1c18b2Aa" class="section"{\textgreater}{\textless}h3 class="abstractTitle text-title my-1" id="d712e8"{\textgreater}Materials and methods{\textless}/h3{\textgreater}{\textless}p{\textgreater}We systematically searched three databases ({\textless}em{\textgreater}PubMed, Scopus{\textless}/em{\textgreater} and {\textless}em{\textgreater}Web of Science{\textless}/em{\textgreater}) for articles on cancer incidence and SES. Qualitative synthesis was performed on the 91 included English language studies, published between 2000 and 2019 in Europe, which focused on adults, relied on cancer registry data and reported on relative risk (RR) estimates.{\textless}/p{\textgreater}{\textless}/div{\textgreater}{\textless}div id="j\_raon-2020-0008\_s\_006\_w2aab3b8c76b1b7b1aab1c18b3Aa" class="section"{\textgreater}{\textless}h3 class="abstractTitle text-title my-1" id="d712e18"{\textgreater}Results{\textless}/h3{\textgreater}{\textless}p{\textgreater}Adults with low SES have increased risk of head and neck, oesophagogastric, liver and gallbladder, pancreatic, lung, kidney, bladder, penile and cervical cancers (highest RRs for lung, head and neck, stomach and cervix). Conversely, high SES is linked with increased risk of thyroid, breast, prostate and skin cancers. Central nervous system and haematological cancers are not associated with SES. The positive gap in testicular cancer has narrowed, while colorectal cancer shows a varying pattern in different countries. Negative associations are generally stronger for men compared to women.{\textless}/p{\textgreater}{\textless}/div{\textgreater}{\textless}div id="j\_raon-2020-0008\_s\_007\_w2aab3b8c76b1b7b1aab1c18b4Aa" class="section"{\textgreater}{\textless}h3 class="abstractTitle text-title my-1" id="d712e21"{\textgreater}Conclusions{\textless}/h3{\textgreater}{\textless}p{\textgreater}In Europe, cancers in almost all common locations are associated with SES and the inequalities can be explained to a varying degree by known life-style related factors, most notably smoking. Independent effects of many individual and area SES measures which capture different aspects of SES can also be observed.{\textless}/p{\textgreater}{\textless}/div{\textgreater}{\textless}/section{\textgreater}},
	language = {en},
	number = {1},
	urldate = {2020-06-12},
	journal = {Radiology and Oncology},
	author = {Mihor, Ana and Tomsic, Sonja and Zagar, Tina and Lokar, Katarina and Zadnik, Vesna},
	month = feb,
	year = {2020},
	note = {Publisher: Sciendo
Section: Radiology and Oncology},
	pages = {1--13},
	file = {Snapshot:/Users/elenadudukina/Zotero/storage/Y3R4RQYN/article-p1.html:text/html;Full Text PDF:/Users/elenadudukina/Zotero/storage/N8RZDELX/Mihor et al. - 2020 - Socioeconomic inequalities in cancer incidence in .pdf:application/pdf}
}

