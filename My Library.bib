	keywords = {Female, Humans, Cohort Studies, Adult, Middle Aged, Pregnancy, Young Adult, Denmark/epidemiology, Incidence, Risk Assessment, Proportional Hazards Models, Stillbirth, Hypertension, Pregnancy-Induced/*epidemiology, Parity, *Disease Susceptibility, Blood Pressure, Hypertension/diagnosis/*epidemiology/prevention \& control},
	pages = {j3078},
