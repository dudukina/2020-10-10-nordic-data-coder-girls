	keywords = {Female, Humans, Male, Aged, Middle Aged, Young Adult, Risk, cancer, Incidence, Child, Adolescent, Age of Onset, Japan/epidemiology, anti-TNF-alpha, Asian Continental Ancestry Group, Colorectal Neoplasms/*epidemiology/*etiology, Crohn Disease/*complications/*epidemiology, Crohn's disease, Japanese, Leukemia, Myeloid, Acute/*epidemiology/*etiology, standardized incidence ratio (SIR)},
	pages = {1300--5},
