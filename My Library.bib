	pmid = {24566677},
	keywords = {Female, Humans, Male, Risk Factors, Adult, Pregnancy, Prospective Studies, Child, Risk Assessment, Denmark, Infant, Surveys and Questionnaires, Child, Preschool, Mothers, Proportional Hazards Models, Prenatal Exposure Delayed Effects, Attention Deficit Disorder with Hyperactivity, Mental Disorders, Acetaminophen, Hyperkinesis},
	pages = {313--320},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/GXMWKPYJ/Liew et al. - 2014 - Acetaminophen use during pregnancy, behavioral pro.pdf:application/pdf}
}

