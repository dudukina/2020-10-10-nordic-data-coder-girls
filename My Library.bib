	urldate = {2020-06-12},
	journal = {The Lancet},
	author = {Danaei, Goodarz and Hoorn, Stephen Vander and Lopez, Alan D. and Murray, Christopher JL and Ezzati, Majid},
	month = nov,
	year = {2005},
	pmid = {16298215},
	note = {Publisher: Elsevier},
	pages = {1784--1793},
