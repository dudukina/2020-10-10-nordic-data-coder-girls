	author = {Sørensen, Gitte Vrelits and Ganz, Patricia A. and Cole, Steven W. and Pedersen, Lars A. and Sørensen, Henrik Toft and Cronin-Fenton, Deirdre P. and Garne, Jens Peter and Christiansen, Peer M. and Lash, Timothy L. and Ahern, Thomas P.},
