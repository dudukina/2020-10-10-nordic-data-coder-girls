	year = {2016},
	pmcid = {PMC5013884},
	keywords = {Humans, Aged, Aged, 80 and over, Survival Analysis, *Bias, Middle Aged, Computer Simulation, Linear Models, Proportional Hazards Models, Selection Bias, *selection bias, *Cognitive Aging, *cognitive decline, *collider-stratification bias, *dementia, *selective survival, *simulation, *survival bias, *truncation by death, Cognitive Dysfunction/*epidemiology/mortality},
	pages = {378--87},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/WQLX28HI/Mayeda et al. - 2016 - A Simulation Platform for Quantifying Survival Bia.pdf:application/pdf}
}

