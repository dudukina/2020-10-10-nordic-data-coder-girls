	keywords = {Female, Humans, Male, Pregnancy, Sweden, Biomarkers/blood, Cross-Sectional Studies, Environmental Exposure/adverse effects/*analysis, Prenatal Care, Semen/*drug effects, Sperm Count, Greenland, Poland, Ukraine, Environmental Pollutants/*adverse effects/blood, Fertility/*drug effects, Gas Chromatography-Mass Spectrometry, Hydrocarbons, Chlorinated/blood/*toxicity, Spouses},
