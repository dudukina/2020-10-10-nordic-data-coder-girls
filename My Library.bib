	year = {2019},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1002/sim.8206},
	keywords = {survival analysis, observational studies, propensity score, inverse probability weighting, restricted mean survival time, time-to-event data},
	pages = {3832--3860},
	file = {Full Text PDF:/Users/elenadudukina/Zotero/storage/7K5KHE92/Conner et al. - 2019 - Adjusted restricted mean survival times in observa.pdf:application/pdf;Snapshot:/Users/elenadudukina/Zotero/storage/BSIGYFJV/sim.html:text/html}
