	keywords = {Humans, *Monte Carlo Method, *Survival Analysis, *Proportional Hazards Models, survival analysis, Propensity Score, Monte Carlo simulations, observational study, inverse probability of treatment weighting (IPTW), variance estimation},
	pages = {5642--5655},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/HMGDDQYT/Austin - 2016 - Variance estimation when using inverse probability.pdf:application/pdf}
}

