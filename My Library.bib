	keywords = {Female, Humans, Cohort Studies, Risk Factors, Pregnancy, Incidence, Time Factors, Risk Assessment, Parity, *Obstetric Labor Complications, Pregnancy, High-Risk, Labor Stage, Third, Netherlands/epidemiology, Placenta, Retained/*complications, Postpartum Hemorrhage/*epidemiology/*etiology},
	pages = {166--72},
