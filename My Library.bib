	journal = {The Lancet Infectious Diseases},
	author = {Dawood, Fatimah S and Iuliano, A Danielle and Reed, Carrie and Meltzer, Martin I and Shay, David K and Cheng, Po-Yung and Bandaranayake, Don and Breiman, Robert F and Brooks, W Abdullah and Buchy, Philippe and Feikin, Daniel R and Fowler, Karen B and Gordon, Aubree and Hien, Nguyen Tran and Horby, Peter and Huang, Q Sue and Katz, Mark A and Krishnan, Anand and Lal, Renu and Montgomery, Joel M and Mølbak, Kåre and Pebody, Richard and Presanis, Anne M and Razuri, Hugo and Steens, Anneke and Tinoco, Yeny O and Wallinga, Jacco and Yu, Hongjie and Vong, Sirenda and Bresee, Joseph and Widdowson, Marc-Alain},
	month = sep,
