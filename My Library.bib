	journal = {Diabetes Metab Res Rev},
	author = {Svensson, E. and Berencsi, K. and Sander, S. and Mor, A. and Rungby, J. and Nielsen, J. S. and Friborg, S. and Brandslund, I. and Christiansen, J. S. and Vaag, A. and Beck-Nielsen, H. and Sorensen, H. T. and Thomsen, R. W.},
