	keywords = {Female, Humans, Pregnancy, Incidence, *Pregnancy Outcome, Prenatal Exposure Delayed Effects/*epidemiology, Infant, Newborn, Angiotensin Receptor Antagonists/*adverse effects/therapeutic use, Angiotensin-Converting Enzyme Inhibitors/*adverse effects/therapeutic use, Hypertension/drug therapy, Oligohydramnios/epidemiology, Renal Insufficiency/epidemiology},
	pages = {444--50},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/AGLAWZ2T/Bullo et al. - 2012 - Pregnancy outcome following exposure to angiotensi.pdf:application/pdf}
