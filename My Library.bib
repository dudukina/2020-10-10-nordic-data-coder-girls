@article{calleja-agius_obstetric_2011,
	title = {Obstetric outcome and cytokine levels in threatened miscarriage},
	volume = {27},
	issn = {1473-0766 (Electronic) 0951-3590 (Linking)},
	doi = {10.3109/09513590.2010.487614},
	abstract = {OBJECTIVES: To evaluate the proportion of women with threatened miscarriage (TM) who proceed to miscarriage in a population of single ethnicity and to investigate prospectively their risk of adverse pregnancy outcome in relationship with the cytokines levels in their circulation. METHODS: We conducted a prospective observational study over a period of 1 year of 94 Maltese women presenting with TM at the same hospital and compared their clinical data with those of 564 age-matched controls from the National Obstetric Information System (NOIS) of Malta. Main outcome measures included gestational age and weight at delivery and incidence of adverse pregnancy outcomes. A pilot study was carried out, where in subgroups of 10 women with TM (n=10), non-pregnant women (n=12), normal pregnant controls (n=9) and women presenting with missed-miscarriage (n=11), the plasma levels of beta-human chorionic gonadotrophin (beta-hCG), tumour necrosis factor alpha (TNFalpha), interferon gamma (IFNgamma), interleukin-6 (IL-6), interleukin-10 (IL-10) and TNF-receptors 1 (R1) and 2 (R2) were measured. RESULTS: Of the women presenting with TM, 25 (26.6\%) proceeded to complete miscarriage. The TM group had also a significantly higher incidence of antepartum haemorrhage (p{\textless}0.005), pre-eclampsia (p{\textless}0.05), foetal growth restriction (p{\textless}0.05), premature labour (p{\textless}0.001) and retained placenta (p{\textless}0.005). In the pilot biochemical analysis, significantly (p{\textless}0.05) higher levels of TNFalpha and lower levels of TNFR2 were found in the TM subgroup compared to non-pregnant controls. The ratio TNFalpha/IL-10 was significantly (p{\textless}0.05) higher and the beta-hCG levels was significantly lower (p{\textless}0.01) in missed-miscarriage and non-pregnant subgroups than in TM and normal pregnant controls. The IFNgamma/1L-10 and IFNgamma/1L-6 ratio were significantly ({\textless}0.001) different between the four subgroups with the lowest level found in TM. No similar gradient was found for the TNFalpha/1L-6 ratio. CONCLUSION: Women presenting with TM are at significantly increased risk of adverse pregnancy outcome and the pathophysiology of these conditions involves a change in the Th1/Th2 balance. Changes in levels of cytokines could help to predict and thus prevent the development of some of these complications.},
	number = {2},
	journal = {Gynecol Endocrinol},
	author = {Calleja-Agius, J. and Schembri-Wismayer, P. and Calleja, N. and Brincat, M. and Spiteri, D.},
	month = feb,
	year = {2011},
	keywords = {Female, Humans, Adult, Pregnancy, Case-Control Studies, *Pregnancy Outcome, Cytokines/*blood, Interleukin-10/blood, Interleukin-6/blood, Pilot Projects, Delivery, Obstetric/*statistics \& numerical data, Abortion, Spontaneous/blood/epidemiology, Abortion, Threatened/*blood/*epidemiology/therapy, Interferon-gamma/blood, Live Birth/epidemiology, Malta/epidemiology, Obstetric Labor Complications/epidemiology, Tumor Necrosis Factor-alpha/blood},
