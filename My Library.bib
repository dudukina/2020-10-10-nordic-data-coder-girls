	journal = {JAMA},
	author = {Sujan, A. C. and Rickert, M. E. and Oberg, A. S. and Quinn, P. D. and Hernandez-Diaz, S. and Almqvist, C. and Lichtenstein, P. and Larsson, H. and D'Onofrio, B. M.},
