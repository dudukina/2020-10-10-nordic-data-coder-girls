	pmcid = {PMC4877414},
	keywords = {Humans, Probability, *Data Interpretation, Statistical, *Confidence Intervals, *Hypothesis testing, *Null testing, *P value, *Power, *Significance tests, *Statistical testing},
	pages = {337--50},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/6ZAEBPQC/Greenland et al. - 2016 - Statistical tests, P values, confidence intervals,.pdf:application/pdf}
