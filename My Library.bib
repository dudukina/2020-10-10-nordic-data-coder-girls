	keywords = {Female, Humans, Male, Risk Factors, cohort study, Incidence, Denmark, Infant, Infant, Newborn, Follow-Up Studies, Gestational Age, Infant, Premature, Apgar Score, Cerebral Hemorrhage, cerebral palsy, Cerebral Palsy, EPIDEMIOLOGY, neurodevelopmental disorder, PERINATOLOGY, respiratory distress syndrome, Respiratory Distress Syndrome, Newborn},
