	keywords = {Female, Humans, Risk Factors, Adult, Pregnancy, Case-Control Studies, Odds Ratio, Denmark, Abortion, Spontaneous/*blood/immunology/*microbiology, Antibodies, Bacterial/*blood, Coxiella burnetii/*immunology, Enzyme-Linked Immunosorbent Assay, Fluorescent Antibody Technique},
	pages = {e31909},
