	keywords = {Avortement spontane, Endometriose, Endometriosis, Fausse couche spontanee, Fetal loss, Grossesse spontanee, Miscarriage, Perte foetale, Spontaneous abortion, Spontaneous pregnancy},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/ZAF96UVU/Minebois et al. - 2017 - [Endometriosis and miscarriage Systematic review].pdf:application/pdf}
