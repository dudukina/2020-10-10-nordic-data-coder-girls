	keywords = {Female, Humans, Male, Cohort Studies, Risk, Denmark/epidemiology, Child, Registries, *Siblings, Prevalence, Proportional Hazards Models, Recurrence, Child Development Disorders, Pervasive/*epidemiology/etiology/genetics},
