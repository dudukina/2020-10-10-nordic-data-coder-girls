	pages = {223--33},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/KQWS3G6P/Falanga et al. - 2013 - Coagulation and cancer biological and clinical as.pdf:application/pdf}
}

@article{hernandez-diaz_fetal_2017,
