	urldate = {2020-09-29},
	journal = {Pediatrics},
	author = {Bateman, Brian T. and Patorno, Elisabetta and Desai, Rishi J. and Seely, Ellen W. and Mogun, Helen and Maeda, Ayumi and Fischer, Michael A. and Hernandez-Diaz, Sonia and Huybrechts, Krista F.},
	month = sep,
	year = {2016},
	pmid = {27577580},
	pmcid = {PMC5005024},
	file = {PubMed Central Full Text PDF:/Users/elenadudukina/Zotero/storage/5ZYEHWEJ/Bateman et al. - 2016 - Late Pregnancy β Blocker Exposure and Risks of Neo.pdf:application/pdf}
