	pages = {692--694},
	file = {Rudolph et al. - 2020 - Defining and Identifying Per-protocol Effects in R.pdf:/Users/elenadudukina/Zotero/storage/JJQTGYJQ/Rudolph et al. - 2020 - Defining and Identifying Per-protocol Effects in R.pdf:application/pdf;Snapshot:/Users/elenadudukina/Zotero/storage/BVB6TZS3/Defining_and_Identifying_Per_protocol_Effects_in.13.html:text/html}
