	pmcid = {PMC4943074},
	keywords = {Female, Humans, Male, Cohort Studies, Retrospective Studies, Young Adult, Risk, Child, Adolescent, Attention Deficit Disorder with Hyperactivity/complications/*drug therapy, Child, Preschool, Central Nervous System Stimulants/*adverse effects, Cardiovascular Diseases/*chemically induced/epidemiology},
