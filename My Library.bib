	keywords = {Female, Humans, Disease-Free Survival, Survival Analysis, *Bias, Proportional Hazards Models, Randomized Controlled Trials as Topic/methods, Amenorrhea/physiopathology, Breast Neoplasms/physiopathology/therapy, Clinical Trials as Topic/*methods, Colorectal Neoplasms/genetics, Medical Oncology/*methods, Mutation, Proto-Oncogene Proteins p21(ras), Proto-Oncogene Proteins/genetics, ras Proteins/genetics, Research Design},
	pages = {2963--9},
