	pmcid = {PMC5860620},
	keywords = {fertility, fatty acids, internet, prospective studies, trans fatty acids},
	pages = {60--74},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/AL6FPMB8/Wise et al. - 2018 - Dietary Fat Intake and Fecundability in 2 Preconce.pdf:application/pdf}
}

