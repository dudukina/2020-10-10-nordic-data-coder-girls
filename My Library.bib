	keywords = {Humans, *Bias, Data Interpretation, Statistical, Logistic Models, *Confounding Factors (Epidemiology), Case-Control Studies, Computer Simulation, *Siblings, Linear Models, *Matched-Pair Analysis, Twin Studies as Topic/*methods/statistics \& numerical data},
	pages = {713--20},
	file = {Frisell et al. - 2012 - Sibling comparison designs bias from non-shared c.pdf:/Users/elenadudukina/Zotero/storage/2SRMRV63/Frisell et al. - 2012 - Sibling comparison designs bias from non-shared c.pdf:application/pdf}
}

@article{dormuth_statin_2009,
	title = {Statin adherence and risk of accidents: a cautionary tale},
	volume = {119},
	issn = {1524-4539 (Electronic) 0009-7322 (Linking)},
	doi = {10.1161/CIRCULATIONAHA.108.824151},
	abstract = {BACKGROUND: Bias in studies of preventive medications can occur when healthier patients are more likely to initiate and adhere to therapy than less healthy patients. We sought evidence of this bias by examining associations between statin exposure and various outcomes that should not be causally affected by statin exposure, such as workplace and motor vehicle accidents. METHODS AND RESULTS: We conducted a prospective cohort study of statin patients using data from British Columbia, Canada, a multiethnic society with a population of 4.3 million people. Study subjects were 141 086 patients who initiated statins for primary prevention. We examined the association between adherence and multiple outcomes such as accidents and screening procedures using multivariable-adjusted Cox proportional hazards models. The study population was 49\% female and had an average age of 61 years. The results from our multivariable-adjusted models showed that more adherent patients were less likely to have accidents than less adherent patients. This effect was greatest for motor vehicle accidents (hazard ratio, 0.75; 95\% confidence interval, 0.72 to 0.79) and workplace accidents (hazard ratio, 0.77; 95\% confidence interval, 0.74 to 0.81). More adherent patients had a greater likelihood of using screening services (hazard ratio, 1.17; 95\% confidence interval, 1.15 to 1.20) and a lower likelihood of developing other diseases likely to be unrelated to a biological affect of a statin (hazard ratio, 0.87; 95\% confidence interval, 0.86 to 0.89). CONCLUSIONS: Our study contributes compelling evidence that patients who adhere to statins are systematically more health seeking than comparable patients who do not remain adherent. Caution is warranted when interpreting analyses that attribute surprising protective effects to preventive medications.},
	number = {15},
	journal = {Circulation},
	author = {Dormuth, C. R. and Patrick, A. R. and Shrank, W. H. and Wright, J. M. and Glynn, R. J. and Sutherland, J. and Brookhart, M. A.},
