	pmcid = {PMC6431068},
	keywords = {Epidemiology, Breast cancer, Breast cancer recurrence, Breast cancer survival},
	pages = {44},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/JU7GL23N/Falstie-Jensen et al. - 2019 - Hypothyroidism and the risk of breast cancer recur.pdf:application/pdf}
