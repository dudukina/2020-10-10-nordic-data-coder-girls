	keywords = {Humans, Animals, Clinical Trials as Topic, Substance-Related Disorders, Drug Evaluation, Preclinical, Euphoria, Illicit Drugs, Pregabalin, Prescription Drug Misuse},
	pages = {9--25},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/MFJ6SCXF/Schjerning et al. - 2016 - Abuse Potential of Pregabalin A Systematic Review.pdf:application/pdf}
