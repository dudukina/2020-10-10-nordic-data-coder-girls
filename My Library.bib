	keywords = {Female, Humans, Cohort Studies, Pregnancy, Prospective Studies, Case-Control Studies, Fetal Development/*drug effects, Infant, Newborn, Gestational Age, Prenatal Exposure Delayed Effects/*chemically induced/epidemiology, *Pregnancy Outcome/epidemiology, Abnormalities, Drug-Induced/epidemiology/etiology, Birth Weight/drug effects, effects/therapeutic use, Hydroxymethylglutaryl-CoA Reductase Inhibitors/administration \& dosage/*adverse},
	pages = {175--7},
