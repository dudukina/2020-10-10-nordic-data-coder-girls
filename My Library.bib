@article{maslova_dietary_2015,
	title = {Dietary protein-to-carbohydrate ratio and added sugar as determinants of excessive gestational weight gain: a prospective cohort study},
	volume = {5},
	issn = {2044-6055},
	doi = {10.1136/bmjopen-2014-005839},
	abstract = {OBJECTIVE: To examine the relation between the protein:carbohydrate (P/C) ratio and added sugar intake in pregnancy and gestational weight gain (GWG). DESIGN: A prebirth cohort including 103 119 pregnancies enrolled between 1996 and 2003. SETTING: All women in Denmark were eligible to participate if they spoke Danish and were planning to carry to term.The pregnant women were recruited and enrolled during their first antenatal visit (6-10 weeks of gestation). PARTICIPANTS: Participants included women with live-born singletons and complete data on dietary intake and GWG, leaving 46 262 women for the analysis. EXPOSURE: Macronutrient intake was quantified using a validated food frequency questionnaire administered in the 25th week of gestation. The P/C ratio and added sugar intake were examined in quintiles. PRIMARY OUTCOME MEASURES: GWG was based on self-reported weight in gestational weeks 12 and 30 and defined as gain in g/week. We used multivariable linear regression, including adjusting for pre-pregnancy body mass index, to calculate relative change in GWG and 95\% CI. RESULTS: Average GWG was 471(224) g/week. The adjusted weight gain was 16 g/week lower (95\% CI 9 to 22, p for trend {\textless}0.001) in the highest (Q5) versus lowest (Q1) quintile of the P/C ratio ( approximately 3\% average reduction across the entire pregnancy). Weight gain for those with {\textgreater}20\%E vs {\textless}12\%E from protein was 36 g/week lower (95\% CI 20 to 53, p for trend {\textless}0.0001; approximately 8\% average reduction). A high P/C ratio was inversely related to intake of added sugars. Added sugar consumption was strongly associated with GWG (Q5 vs Q1: 34, 95\% CI 28 to 40 g/week, p for trend {\textless}0.0001). CONCLUSIONS: A high P/C ratio was associated with reduced GWG. This association appeared to be partly driven by a decrease in intake of added sugar. These results are consistent with randomised trials in non-pregnant participants. A dietary intervention targeting an increased P/C ratio with emphasis on reducing added sugar can contribute to reducing excessive GWG.},
	language = {eng},
	number = {2},
	journal = {BMJ Open},
	author = {Maslova, E. and Halldorsson, T. I. and Astrup, A. and Olsen, S. F.},
	month = feb,
	year = {2015},
	pmcid = {PMC4325128},
	keywords = {Female, Humans, Adult, Pregnancy, Prospective Studies, Denmark, Body Mass Index, Epidemiology, *Dietary Sucrose/administration \& dosage/adverse effects, *Feeding Behavior, *Weight Gain, Dietary Carbohydrates/administration \& dosage/adverse effects, Dietary Proteins/*administration \& dosage, Nutrition \& dietetics, Obesity/etiology/*prevention \& control, Obstetrics, Pregnancy Complications/etiology/*prevention \& control},
	pages = {e005839},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/PDXXFPTU/Maslova et al. - 2015 - Dietary protein-to-carbohydrate ratio and added su.pdf:application/pdf}
}

@article{geraghty_maternal_2016,
	title = {Maternal {Blood} {Lipid} {Profile} during {Pregnancy} and {Associations} with {Child} {Adiposity}: {Findings} from the {ROLO} {Study}},
	volume = {11},
	issn = {1932-6203 (Electronic) 1932-6203 (Linking)},
	doi = {10.1371/journal.pone.0161206},
	abstract = {BACKGROUND: The in-utero environment affects fetal development; it is vital to understand how maternal diet during pregnancy influences childhood body composition. While research indicates that triglycerides in hyperglycaemic women may increase birth weight, little is known about this relationship in euglycemic women. This study examines the relationship between maternal blood lipid status and infant adiposity up to 2 years of age. METHODS: Data from 331 mother-child pairs from the ROLO longitudinal birth cohort study was analysed. Maternal dietary intakes were recorded and fasting blood lipids, leptin and HOMA were measured in early and late pregnancy and cord blood. Infant anthropometric measurements and skin-fold thicknesses were recorded at birth, 6 months and 2 years. Correlation and regression analyses were used to explore associations between maternal blood lipid status and infant adiposity. RESULTS: All maternal blood lipids increased significantly during pregnancy. Maternal dietary fat intake was positively associated with total cholesterol levels in early pregnancy. Late pregnancy triglycerides were positively associated with birth weight (P = 0.03) while cord blood triglycerides were negatively associated with birth weight (P = 0.01). Cord HDL-C was negatively associated with infant weight at 6 months (P = 0.005). No other maternal blood lipids were associated with infant weight or adiposity up to 2 years of age. CONCLUSION: Maternal and fetal triglycerides were associated with birth weight and cord HDL-C with weight at 6 months. Thus, maternal lipid concentrations may exert in-utero influences on infant body composition. There may be potential to modulate infant body composition through alteration of maternal diet during pregnancy.},
	number = {8},
	journal = {PLoS One},
	author = {Geraghty, A. A. and Alberdi, G. and O'Sullivan, E. J. and O'Brien, E. C. and Crosbie, B. and Twomey, P. J. and McAuliffe, F. M.},
	year = {2016},
	pmcid = {PMC4999287},
	keywords = {Female, Humans, Cohort Studies, Adult, Pregnancy, Regression Analysis, Infant, Infant, Newborn, Mothers, Body Mass Index, Longitudinal Studies, Birth Weight, Lipids/*blood, *Adiposity, *Body Composition, Anthropometry, Blood Glucose, Cholesterol, HDL/blood, Fetal Blood/metabolism, Hyperglycemia/*blood, Overweight, Skinfold Thickness},
	pages = {e0161206},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/HQWCXFEC/Geraghty et al. - 2016 - Maternal Blood Lipid Profile during Pregnancy and .pdf:application/pdf}
}

