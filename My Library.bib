	keywords = {Female, Humans, Risk Factors, Adult, Prospective Studies, Case-Control Studies, Odds Ratio, Adolescent, Registries, *Abortion, Spontaneous, Sweden/epidemiology, Medical Records, Abortion, Induced/*adverse effects, Breast Neoplasms/epidemiology/*etiology},
	pages = {676--9},
