@article{sellier_decreasing_2016,
	title = {Decreasing prevalence in cerebral palsy: a multi-site {European} population-based study, 1980 to 2003},
	volume = {58},
	copyright = {© 2015 Mac Keith Press},
	issn = {1469-8749},
	shorttitle = {Decreasing prevalence in cerebral palsy},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1111/dmcn.12865},
	doi = {10.1111/dmcn.12865},
	abstract = {Aim To monitor the trends in prevalence of cerebral palsy (CP) by birthweight in Europe, 1980 to 2003. Method Data were collated from 20 population-based registers contributing to the Surveillance of Cerebral Palsy in Europe database. Trend analyses were conducted in four birthweight groups: {\textless}1000g (extremely low birthweight [ELBW]); 1000 to 1499g (very low birthweight [VLBW]); 1500 to 2499g (moderately low birthweight [MLBW]); and {\textgreater}2499g (normal birthweight [NBW]). Results The overall prevalence of CP decreased from 1.90 to 1.77 per 1000 live births, p{\textless}0.001, with a mean annual fall of 0.7\% (95\% confidence interval [CI] −0.3\% to −1.0\%). Prevalence in NBW children showed a non-significant trend from 1.17 to 0.89 per 1000 live births (p=0.22). Prevalence in MLBW children decreased from 8.5 to 6.2 per 1000 live births (p{\textless}0.001), but not linearly. Prevalence in VLBW children also declined from 70.9 to 35.9 per 1000 live births (p{\textless}0.001) with a mean annual fall of 3.4\% (95\% CI −2.4\% to −4.3\%). Prevalence in ELBW children remained stable, at a mean rate of 42.4 per 1000 live births. Interpretation The decline in prevalence of CP in children of VLBW continues, and confirms that previously reported. For the first time, there is also a significant decline among those of MLBW, resulting in a significant overall decrease in the prevalence of CP.},
	language = {en},
	number = {1},
	urldate = {2020-08-23},
	journal = {Developmental Medicine \& Child Neurology},
	author = {Sellier, Elodie and Platt, Mary Jane and Andersen, Guro L. and Krägeloh‐Mann, Ingeborg and Cruz, Javier De La and Cans, Christine},
	year = {2016},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1111/dmcn.12865},
	pages = {85--92},
	file = {Full Text PDF:/Users/elenadudukina/Zotero/storage/U7U82U73/Sellier et al. - 2016 - Decreasing prevalence in cerebral palsy a multi-s.pdf:application/pdf}
}

@article{topp_trend_1997,
	title = {Trend in cerebral palsy birth prevalence in eastern {Denmark}: birth-year period 1979–86},
	volume = {11},
	issn = {1365-3016},
	shorttitle = {Trend in cerebral palsy birth prevalence in eastern {Denmark}},
	url = {https://onlinelibrary.wiley.com/doi/abs/10.1046/j.1365-3016.1997.d01-24.x},
	doi = {10.1046/j.1365-3016.1997.d01-24.x},
	abstract = {To investigate changes in cerebral palsy birth prevalence and perinatal mortality rate by different gestational age groups, 1979–86, cerebral palsy cases in eastern Denmark were identified from the Danish Cerebral Palsy Register, and information on birth and mortality rates was sought in the Danish Medical Birth Register. From 1979–82 to 1983–86, the birth prevalence of cerebral palsy increased from 2.6 to 3.0 per 1000 (P {\textless} 0.05). The rate for infants of 31 weeks' gestation or more did not change, whereas a significant increase was observed in infants below 31 weeks (85–123 per 1000, P {\textless} 0.05). In the same periods, perinatal mortality in eastern Denmark decreased significantly from 8.6 to 7.8 per 1000. The decrease in stillbirth rate was significant in all subgroups of gestational ages except in those of 28–30 weeks' gestation. The early neonatal mortality rate decreased significantly only in infants below 31 weeks (282–239 per 1000, P {\textless} 0.05). Thus, in eastern Denmark, cerebral palsy birth prevalence has increased from birth-year period 1979–82 to 1983–86 because of an increased rate in preterm infants below 31 weeks, who at the same time had a reduced risk of early neonatal death.},
	language = {en},
	number = {4},
	urldate = {2020-08-23},
	journal = {Paediatric and Perinatal Epidemiology},
	author = {Topp, Monica and Uldall, Peter and Langhoff‐Roos, Jens},
	year = {1997},
	note = {\_eprint: https://onlinelibrary.wiley.com/doi/pdf/10.1046/j.1365-3016.1997.d01-24.x},
	pages = {451--460},
	file = {Full Text PDF:/Users/elenadudukina/Zotero/storage/H4K4RL6X/Topp et al. - 1997 - Trend in cerebral palsy birth prevalence in easter.pdf:application/pdf}
}

@article{hoei-hansen_decline_2019,
	title = {Decline in severe spastic cerebral palsy at term in {Denmark} 1999–2007},
	volume = {23},
	issn = {1090-3798},
	url = {http://www.sciencedirect.com/science/article/pii/S1090379818300308},
	doi = {10.1016/j.ejpn.2018.08.010},
	abstract = {Aim
To analyse trends in prevalence and severity of cerebral palsy (CP) in Denmark in birth years 1999–2007 and compare with previous periods.
Method
Data has been collected uniformly in the Danish cerebral palsy national register nationwide since 1995. Rates in the time periods 1999–2001, 2002–2004 and 2005–2007 covering 585,393 births were analysed by gestational age and subtypes.
Results
Total number of CP cases in the period was 1165. The overall prevalence of CP decreased significantly from 2.1 in 1999–2001 to 1.8 in 2005–2007 per 1000 livebirths (p = 0.022). The decline was only significant for children born at term (p = 0.007) but not for the preterm (p = 0.44). The decline in children born at term was based on a decrease in bilateral spastic CP (n = 117 in years 1999–2001 and n = 59 in 2005–2007). Multidisciplinary obstetric skills training with neonatal resuscitation in Denmark was initiated in 2003 and timely associated with the decrease. The prevalence of unilateral spastic CP the prevalence did not change, but in the two last time periods more children had a right-sided than left-sided unilateral spastic CP.
Conclusion
The decline in rate of CP seen in 2005–2007 as compared to 1999–2001 was mainly based on fewer cases of severe spastic CP in term infants. We hypothesize that improved neonatal resuscitation in the delivery room may be partly responsible for the decrease. In premature children the decline was not significant in this time period, but has been dramatically decreasing in the years before the time period here analysed.},
	language = {en},
	number = {1},
	urldate = {2020-08-23},
	journal = {European Journal of Paediatric Neurology},
	author = {Hoei-Hansen, Christina Engel and Laursen, Bjarne and Langhoff-Roos, Jens and Rackauskaite, Gija and Uldall, Peter},
	month = jan,
	year = {2019},
	note = {tex.ids: hoei-hansen\_decline\_2019-1},
	keywords = {Cerebral palsy, Prevalence, Epidemiology, Aetiology, Gestational age},
	pages = {94--101},
	file = {ScienceDirect Full Text PDF:/Users/elenadudukina/Zotero/storage/CGB9QTKH/Hoei-Hansen et al. - 2019 - Decline in severe spastic cerebral palsy at term i.pdf:application/pdf;ScienceDirect Snapshot:/Users/elenadudukina/Zotero/storage/XNXUWBDB/S1090379818300308.html:text/html;ScienceDirect Full Text PDF:/Users/elenadudukina/Zotero/storage/DTHBAAM5/Hoei-Hansen et al. - 2019 - Decline in severe spastic cerebral palsy at term i.pdf:application/pdf;ScienceDirect Snapshot:/Users/elenadudukina/Zotero/storage/3FJIBA7C/S1090379818300308.html:text/html}
}

