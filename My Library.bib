	keywords = {mental health, coronary heart disease, health economics},
	pages = {e018181},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/7PK6H3GI/Zomer et al. - 2017 - Effectiveness and cost-effectiveness of a cardiova.pdf:application/pdf}
