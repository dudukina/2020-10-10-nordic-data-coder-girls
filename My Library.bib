	keywords = {Female, Humans, Retrospective Studies, Adult, Middle Aged, Pregnancy, Prospective Studies, Young Adult, Logistic Models, *Pregnancy Outcome, Adolescent, Surveys and Questionnaires, Multivariate Analysis, Databases, Factual, Socioeconomic Factors, Maternal Age, Psychiatric Status Rating Scales, *Social Support, *Stress, Psychological, Anxiety, Depression, Patient Acceptance of Health Care/*statistics \& numerical data, Pregnancy Complications, Pregnant Women/*psychology, Prenatal Care/*standards, United Kingdom},
	pages = {309},
