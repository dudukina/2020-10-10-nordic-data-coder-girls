	pmcid = {PMC4983650},
	keywords = {Female, Humans, Male, Risk, Incidence, *causal inference, Follow-Up Studies, *antiretroviral therapy, *epidemiologic methods, *Hiv, *intervention studies, *policy, *Probability, *treatment guidelines, Anti-Retroviral Agents/*therapeutic use, CD4 Lymphocyte Count, HIV Infections/drug therapy/*mortality, HIV Seropositivity/*drug therapy, Kaplan-Meier Estimate, Time-to-Treatment, United States/epidemiology},
	pages = {336--44},
