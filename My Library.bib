	pmcid = {PMC5285457},
	keywords = {Female, Humans, Male, Cohort Studies, Adult, Middle Aged, Time Factors, *Confounding Factors (Epidemiology), *Body Weight, Causality, *Statistics as Topic, Algorithms, Smoking Cessation, *Blood Pressure, Smoking/*epidemiology/therapy},
	pages = {266--274},
