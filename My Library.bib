	author = {Scheres, L. and Brekelmans, M. and Ageno, W. and Ay, C. and Buller, H. R. and Eichinger, S. and Hutten, B. A. and Klok, F. A. and Middeldorp, S. and Schreiber, K. and Stach, K. and Blondon, M. and Delluc, A.},
	month = nov,
	year = {2018},
	pmcid = {PMC6221000},
	keywords = {venous thromboembolism, Abnormal uterine bleeding, edoxaban, oral anticoagulants, vaginal bleeding, warfarin},
	pages = {1581--1589},
