	pages = {2429--2437},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/ZX823TSR/Huybrechts et al. - 2018 - Association of Maternal First-Trimester Ondansetro.pdf:application/pdf;Full Text:/Users/elenadudukina/Zotero/storage/UZ4HDWFU/Huybrechts et al. - 2018 - Association of Maternal First-Trimester Ondansetro.pdf:application/pdf}
