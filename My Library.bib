@article{sjolander_confounders_2017,
	title = {Confounders, {Mediators}, or {Colliders}: {What} {Types} of {Shared} {Covariates} {Does} a {Sibling} {Comparison} {Design} {Control} {For}?},
	volume = {28},
	issn = {1531-5487 (Electronic) 1044-3983 (Linking)},
	doi = {10.1097/EDE.0000000000000649},
	abstract = {The sibling comparison design is an important epidemiologic tool to control for unmeasured confounding, in studies of the causal effect of an exposure on an outcome. It is routinely argued that within-family associations are automatically controlled for all measured and unmeasured covariates that are shared (constant) within sets of siblings, such as early childhood environment and parental genetic makeup. However, an important lesson from modern causal inference theory is that not all types of covariate control are desirable. In particular, it has been argued that collider control always leads to bias, and that mediator control may or may not lead to bias, depending on the research question. In this article, we use directed acyclic graphs (DAGs) to distinguish between shared confounders, shared mediators and shared colliders, and we examine which of these shared covariates the sibling comparison design really controls for.},
	number = {4},
	journal = {Epidemiology},
	author = {Sjolander, A. and Zetterqvist, J.},
	month = jul,
	year = {2017},
	keywords = {Female, Humans, Male, Bias, Data Interpretation, Statistical, *Confounding Factors (Epidemiology), *Siblings, Epidemiologic Methods, Likelihood Functions},
	pages = {540--547},
	file = {sjolander_sibling.pdf:/Users/elenadudukina/Zotero/storage/CGWPTAEK/sjolander_sibling.pdf:application/pdf;Arvid-Sjölander-Family-design-161012.pdf:/Users/elenadudukina/Zotero/storage/4BJSJPQ5/Arvid-Sjölander-Family-design-161012.pdf:application/pdf;Sjolander and Zetterqvist - 2017 - Confounders, Mediators, or Colliders What Types o.pdf:/Users/elenadudukina/Zotero/storage/XMWPYN69/Sjolander and Zetterqvist - 2017 - Confounders, Mediators, or Colliders What Types o.pdf:application/pdf}
}

