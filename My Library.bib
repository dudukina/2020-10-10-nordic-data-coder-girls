	author = {Landres, I. V. and Milki, A. A. and Lathi, R. B.},
	month = may,
	year = {2010},
	keywords = {Female, Humans, Risk Factors, Retrospective Studies, Adult, Pregnancy, Case-Control Studies, Body Mass Index, *Chromosome Aberrations, Abortion, Spontaneous/*etiology/*genetics/pathology, Aneuploidy, Karyotyping, Obesity/*complications/pathology, Overweight/complications/pathology, Polycystic Ovary Syndrome/complications/pathology},
