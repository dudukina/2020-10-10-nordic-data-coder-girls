	keywords = {Female, Humans, Cohort Studies, Risk Factors, Adult, Middle Aged, Pregnancy, Young Adult, Time Factors, Denmark, Proportional Hazards Models, Abortion, Spontaneous/*etiology, Socioeconomic Factors, Maternal Age, Cohort study, Spontaneous abortion, Contraceptives, Oral/*adverse effects, Oral contraceptive},
	pages = {936--41 e1},
