	month = mar,
	year = {1996},
	keywords = {pregnancy, alcohol, epidemiologic methods, fetal death, abnormalities, bias (epidemiology), birth-weight, environmental exposure, life style, occupational exposure, pesticides, smoking},
	pages = {597--607}
}

@article{matok_immortal_2014,
	title = {Immortal time bias in observational studies of drug effects in pregnancy},
	volume = {100},
	issn = {1542-0760 (Electronic) 1542-0752 (Linking)},
	doi = {10.1002/bdra.23271},
	abstract = {BACKGROUND: The use of decongestants during the second or third trimesters of pregnancy has been associated with a decreased risk of preterm delivery in two observational studies. This effect may have been subject to immortal time bias, a bias arising from the improper classification of exposure during follow-up. We illustrate this bias by repeating the studies using a different data source. METHODS: The United Kingdom Hospital Episodes Statistics and the Clinical Practice Research Datalink databases were linked to identify all live singleton pregnancies among women aged 15 to 45 years between 1997 and 2012. Cox proportional hazards models were used to estimate hazard ratios (HRs) and 95\% confidence intervals of preterm delivery (before 37 weeks of gestation) by considering the use of decongestants during the third trimester as a time-fixed (biased analysis which misclassifies unexposed person-time as exposed person-time) and time-varying exposure (unbiased analysis with proper classification of unexposed person-time). All models were adjusted for maternal age, smoking status, maternal diabetes, maternal hypertension, preeclampsia, and parity. RESULTS: Of the 195,582 singleton deliveries, 10,248 (5.2\%) were born preterm. In the time-fixed analysis, the HR of preterm delivery for the use of decongestants was below the null and suggestive of a 46\% decreased risk (adjusted HR = 0.54; 95\% confidence interval, 0.24-1.20). In contrast, the HR was closer to null (adjusted HR = 0.93 95\% confidence interval, 0.42-2.06) when the use of decongestants was treated as a time-varying variable. CONCLUSION: Studies of drug safety in pregnancy should use the appropriate statistical techniques to avoid immortal time bias, particularly when the exposure occurs at later stages of pregnancy.},
	number = {9},
	journal = {Birth Defects Res A Clin Mol Teratol},
	author = {Matok, I. and Azoulay, L. and Yin, H. and Suissa, S.},
