	note = {tex.ids: forthun\_maternal\_2016-1},
	keywords = {Female, Humans, Cohort Studies, Adult, Pregnancy, Young Adult, Risk, Denmark/epidemiology, Denmark, Norway, Prevalence, *Body Mass Index, Mothers, Body Mass Index, Norway/epidemiology, Overweight, *Mothers, Cerebral Palsy/*epidemiology, Overweight/*epidemiology, Cerebral Palsy},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/B2N9WZTN/Forthun et al. - 2016 - Maternal Prepregnancy BMI and Risk of Cerebral Pal.pdf:application/pdf;Full Text:/Users/elenadudukina/Zotero/storage/9GJJWM42/Forthun et al. - 2016 - Maternal Prepregnancy BMI and Risk of Cerebral Pal.pdf:application/pdf}
