	keywords = {Humans, *Research Design, *Data Interpretation, Statistical, *epidemiologic methods, *Probability, Predictive Value of Tests, *epidemiologic research design, *null hypothesis, *Reproducibility of Results, *significance testing, *statistical significance, *statistics, False Positive Reactions, Research/*standards},
	pages = {627--635},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/U9DG374H/Lash - 2017 - The Harm Done to Reproducibility by the Culture of.pdf:application/pdf}
}

