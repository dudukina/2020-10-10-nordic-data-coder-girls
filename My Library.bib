@article{podymow_preconception_2015,
	title = {Preconception and pregnancy management of women with diabetic nephropathy on angiotensin converting enzyme inhibitors},
	volume = {83},
	issn = {0301-0430 (Print) 0301-0430 (Linking)},
	doi = {10.5414/CN108391},
	abstract = {Angiotensin converting enzyme (ACE) inhibitors are the mainstay of treatment for diabetic nephropathy to slow progression of disease. Diabetic women of childbearing age with nephropathy should be treated with ACE inhibitors as per guidelines in the pre-pregnancy period. ACE inhibitor use and exposure in the first trimester is controversial and requires counselling pre-pregnancy regarding the risks and benefits of use up to the first trimester, as well as the need to stop ACE inhibitors prior to the second trimester. Current evidence does not suggest that ACE inhibitors in the first trimester are associated with a greater risk of fetal malformations when compared to other antihypertensives. This topic is reviewed in depth, along with blood pressure targets in pregnant women with diabetic proteinuric disease, evidence for prevention of pre-eclampsia, self-monitoring of blood pressures at home in the latter half of pregnancy and the signs and symptoms of pre-eclampsia, proteinuria evolution in pregnancy, renal function prognosis, and restarting ACE inhibitors when breast feeding in the post-partum period.},
