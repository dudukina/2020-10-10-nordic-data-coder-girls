	pmcid = {PMC4099475},
	keywords = {Humans, Cohort Studies, Risk Factors, Prevalence, Cerebral Palsy/epidemiology/*genetics, Diseases in Twins/epidemiology/genetics, Norway/epidemiology, Pedigree},
