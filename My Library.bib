	keywords = {Female, Humans, Risk Factors, Pregnancy, *Pregnancy Outcome, Infant, Newborn, Multivariate Analysis, Gestational Age, Birth Weight, Hysterectomy, Hypertension/complications, Severity of Illness Index, Obstetric Labor Complications, Arteries/injuries, Blood Transfusion, Iliac Artery/injuries, Labor Stage, Second, Labor, Induced/adverse effects, Oxytocics/adverse effects, Oxytocin/adverse effects, Postpartum Hemorrhage/*etiology/therapy, Uterus/blood supply, Vacuum Extraction, Obstetrical/adverse effects},
	pages = {149--54},
