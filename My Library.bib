	pmcid = {PMC5669007},
	keywords = {Female, Humans, Adult, Pregnancy, Prospective Studies, *Pregnancy Outcome, Prevalence, *Obesity, Gestational Age, Predictive Value of Tests, Pre-Eclampsia/epidemiology, Diabetes, Gestational/epidemiology, ROC Curve, *Birth, *Obesity/epidemiology, *Prediction, *Pregnancy Complications/epidemiology, *Risk stratification, *Uncomplicated pregnancy, Adiponectin},
	pages = {194},
