	month = may,
	year = {2006},
	pmcid = {PMC2715942},
	keywords = {Female, Humans, Male, Aged, Aged, 80 and over, Cohort Studies, Treatment Outcome, Contraindications, *Drug Prescriptions, *Practice Patterns, Physicians', Anti-Inflammatory Agents, Non-Steroidal/therapeutic use, Cyclooxygenase 2 Inhibitors/therapeutic use, Gastrointestinal Tract/drug effects, Pennsylvania, Pharmacoepidemiology/*statistics \& numerical data},
	pages = {268--75},
