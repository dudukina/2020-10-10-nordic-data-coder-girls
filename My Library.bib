	pmcid = {PMC5838577},
	pages = {176--184},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/GWKQRJHP/Levine et al. - 2018 - Association of Maternal Use of Folic Acid and Mult.pdf:application/pdf}
