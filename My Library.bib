	journal = {Paediatric and Perinatal Epidemiology},
	author = {Brix, Nis and Ernst, Andreas and Lauridsen, Lea Lykke Braskhøj and Parner, Erik Thorlund and Arah, Onyebuchi A. and Olsen, Jørn and Henriksen, Tine Brink and Ramlau‐Hansen, Cecilia Høst},
