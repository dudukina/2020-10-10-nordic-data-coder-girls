	keywords = {Female, Humans, Risk Factors, Pregnancy, Infant, Newborn, Pregnancy Outcome, *Fetal Growth Retardation/epidemiology/etiology, *Hypertension, Pregnancy-Induced/epidemiology/etiology, *Pre-Eclampsia/epidemiology/etiology},
	pages = {921--31},
