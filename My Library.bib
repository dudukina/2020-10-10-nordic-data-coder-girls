	keywords = {Female, Humans, Aged, Risk Factors, Adult, Middle Aged, Pregnancy, Child, Adolescent, Parity, Venous Thromboembolism/*epidemiology, Clinical Trials as Topic, Ovariectomy, Postmenopause, *Age Factors, *Estrogen Replacement Therapy/adverse effects, *Menopause, *Reproductive History, *Women's Health, Menarche},
	pages = {214--20},
