	keywords = {Female, Humans, Male, Adult, Pregnancy, pregnancy, Child, acetaminophen, Prenatal Exposure Delayed Effects, attention-deficit/hyperactivity disorder, Confounding Factors, Epidemiologic, negative control exposure analysis, neurological development, prenatal exposure delayed effects, uncontrolled confounding, Attention Deficit Disorder with Hyperactivity, Maternal Exposure, Acetaminophen, Analgesics, Non-Narcotic},
	pages = {768--775},
