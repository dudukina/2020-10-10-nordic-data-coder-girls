@article{levine_association_2018,
	title = {Association of {Maternal} {Use} of {Folic} {Acid} and {Multivitamin} {Supplements} in the {Periods} {Before} and {During} {Pregnancy} {With} the {Risk} of {Autism} {Spectrum} {Disorder} in {Offspring}},
	volume = {75},
