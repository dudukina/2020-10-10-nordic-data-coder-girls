	keywords = {Female, Humans, Risk Factors, Pregnancy, Prospective Studies, *Abortion, Spontaneous, *Occupational Health, *Work Schedule Tolerance, *Workload},
	pages = {325--34},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/NP9WEM92/Bonde et al. - 2013 - Miscarriage and occupational activity a systemati.pdf:application/pdf}
