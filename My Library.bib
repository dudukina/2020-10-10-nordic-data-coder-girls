@article{dahlgren_prenatal_2001,
	title = {Prenatal cytokine exposure results in obesity and gender-specific programming},
	volume = {281},
	issn = {0193-1849 (Print) 0193-1849},
	abstract = {Prenatal events appear to program hormonal homeostasis, contributing to the development of somatic disorders at an adult age. The aim of this study was to examine whether maternal exposure to cytokines or to dexamethasone (Dxm) would be followed by hormonal consequences in the offspring at adult age. Pregnant rats were injected on days 8, 10, and 12 of gestation with either human interleukin-6 (IL-6) or tumor necrosis factor-alpha (TNF-alpha) or with Dxm. Control dams were injected with vehicle. All exposed offspring developed increased body weight (P {\textless} 0.05--0.001), apparently due to an increase of 30--40\% in adipose tissue weight (P {\textless} 0.05--0.01). Corticosterone response to stress was increased in the IL-6 group (P {\textless} 0.05-0.01). Dxm-treated male rats exhibited blunted Dexamethasone suppression test results. In male rats, insulin sensitivity was decreased after IL-6 exposure (P {\textless} 0.01), whereas basal insulin was elevated in the TNF-alpha group (P {\textless} 0.01). In female rats, plasma testosterone levels were higher in all exposed groups compared with controls (P {\textless} 0.01--0.001), with the exception of Dxm-exposed offspring. Males in the TNF-alpha group showed decreased locomotor activity (P {\textless} 0.05), and females in the IL-6 group showed increased locomotor activity (P {\textless} 0.05). These results indicate that prenatal exposure to cytokines or Dxm leads to increased fat depots in both genders. In females, cytokine exposure was followed by a state of hyperandrogenicity. The results suggest that prenatal exposure to cytokines or Dxm can induce gender-specific programming of neuroendocrine regulation with consequences in adult life.},
	language = {eng},
	number = {2},
	journal = {Am J Physiol Endocrinol Metab},
	author = {Dahlgren, J. and Nilsson, C. and Jennische, E. and Ho, H. P. and Eriksson, E. and Niklasson, A. and Bjorntorp, P. and Albertsson Wikland, K. and Holmang, A.},
	month = aug,
