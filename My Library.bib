	pages = {795--802},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/EYYQ877R/Nianogo and Arah - 2018 - Impact of Public Health Interventions on Obesity a.pdf:application/pdf;1-s2.0-S0749379718321275-mmc1.pdf:/Users/elenadudukina/Zotero/storage/6QK9VX6B/1-s2.0-S0749379718321275-mmc1.pdf:application/pdf}
