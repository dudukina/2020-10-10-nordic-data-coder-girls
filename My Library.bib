	keywords = {Female, Humans, Risk Factors, Adult, Incidence, Logistic Models, Multivariate Analysis, Postpartum Period, Anemia/epidemiology/etiology, Diagnostic Errors, Postpartum Hemorrhage/diagnosis/*epidemiology/*etiology},
	pages = {e0190845},
