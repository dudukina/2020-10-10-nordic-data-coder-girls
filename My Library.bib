	year = {2014},
	pmcid = {PMC4285163},
	keywords = {Humans, Bias, Monte Carlo Method, Computer Simulation, Practice Patterns, Physicians', Patient Discharge, *Algorithms, *Biostatistics, computer algorithms, Hypolipidemic Agents/therapeutic use, matching, Monte Carlo simulations, Myocardial Infarction/drug therapy, Ontario, optimal matching, propensity score, propensity-score matching},
