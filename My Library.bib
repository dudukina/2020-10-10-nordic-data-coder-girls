	journal = {BJOG},
	author = {Urquia, M. L. and Glazier, R. H. and Gagnon, A. J. and Mortensen, L. H. and Nybo Andersen, A. M. and Janevic, T. and Guendelman, S. and Thornton, D. and Bolumar, F. and Rio Sanchez, I. and Small, R. and Davey, M. A. and Hjern, A. and Roam Collaboration},
