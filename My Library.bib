	keywords = {Humans, Logistic Models, Odds Ratio, Computer Simulation, Epidemiologic Studies, Child, *Cross-Sectional Studies, *Prevalence, Cluster Analysis},
	pages = {80},
	file = {Full Text:/Users/elenadudukina/Zotero/storage/ZKH6KY6U/Santos et al. - 2008 - Estimating adjusted prevalence ratio in clustered .pdf:application/pdf}
}

@article{ehrenstein_association_2009,
	title = {Association of {Apgar} score at five minutes with long-term neurologic disability and cognitive function in a prevalence study of {Danish} conscripts},
