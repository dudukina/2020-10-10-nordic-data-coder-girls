	keywords = {Female, Humans, Cohort Studies, Risk Factors, Adult, Pregnancy, Adolescent, Infant, Newborn, Pregnancy Outcome, Hypertension/complications, Obesity/complications, Pre-Eclampsia/*etiology, Risk Assessment/methods},
	pages = {412--9},
